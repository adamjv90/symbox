    "keywords": ["vagrant", "puppet", "vm", "virtualbox", "environment"],
    "minimum-stability": "stable",
    "homepage": "https://github.com/basecom/symbox",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon SchrÃ¶er",
            "email": "schroeer@basecom.de",
            "homepage": "http://www.basecom.de/"
        }
    ],
    "require": {
        "symfony/symfony": ">=2.3,<2.4"
    },
    "autoload": {
        "psr-0": { "basecom\\symbox": "" }
    },
    "target-dir": "basecom/symbox",
    "scripts": {
        "post-package-install":  "basecom\\symbox\\ComposerCallback::postPackageInstall",
        "post-package-update":   "basecom\\symbox\\ComposerCallback::postPackageUpdate",
        "pre-package-uninstall": "basecom\\symbox\\ComposerCallback::prePackageUninstall"
    }
}